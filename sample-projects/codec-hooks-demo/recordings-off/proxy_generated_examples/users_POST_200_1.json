{
    "http-request": {
        "path": "/users",
        "method": "POST",
        "headers": {
            "Accept": "*/*",
            "Content-Type": "application/json"
        },
        "body": {
            "name": "Jane Doe",
            "email": "jane@example.com",
            "role": "developer"
        }
    },
    "http-response": {
        "status": 200,
        "body": {
            "id": 123,
            "name": "Jane Doe",
            "email": "jane@example.com",
            "role": "developer"
        },
        "status-text": "OK",
        "headers": {
            "X-Powered-By": "Express",
            "Content-Type": "application/json",
            "Connection": "keep-alive",
            "Keep-Alive": "timeout=5"
        }
    }
}