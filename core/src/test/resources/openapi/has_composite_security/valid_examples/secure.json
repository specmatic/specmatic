{
    "http-request": {
        "path": "/secure",
        "method": "POST",
        "query": {
            "apiKey": "1234"
        },
        "headers": {
            "Content-Type": "application/json",
            "Authorization": "Bearer API-SECRET"
        },
        "body": {
            "message": "Hello to Secure"
        }
    },
    "http-response": {
        "status": 200,
        "body": {
            "message": "Hello from Secure"
        },
        "status-text": "OK",
        "headers": {
            "Content-Type": "application/json"
        }
    }
}